{
  "name": "basic-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "build": "webpack --config webpack.config.js",
    "build:dev": "webpack --config webpack.config.js -w",
    "dev": "concurrently --kill-others \"npm run build:dev\" \"npm run start:dev\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "^4.1.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "waveform-playlist": "^4.0.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.20",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0"
  }
}
